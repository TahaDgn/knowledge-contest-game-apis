{
  "extends": [
    "../../.eslintrc.json"
  ],
  "ignorePatterns": [
    "!**/*"
  ],
  "overrides": [
    {
      "parser": "@typescript-eslint/parser",
      "parserOptions": {
        "project": [
          "**/tsconfig.*.json"
        ],
        "tsconfigRootDir": ".",
        "sourceType": "module"
      },
      "files": [
        "*.ts",
        "*.tsx",
        "*.js",
        "*.jsx"
      ]
    }
  ]
}